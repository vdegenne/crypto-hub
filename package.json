  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "@material/mwc-top-app-bar": "^0.27.0",
    "@material/web": "^2.4.0",
    "@snar/lit": "^1.8.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@vdegenne/about-dialog": "^0.4.2",
    "@vdegenne/binance": "^0.1.7",
    "@vdegenne/cmc": "^0.1.24",
    "@vdegenne/debouncer": "^1.0.6",
    "@vdegenne/forms": "^0.1.50",
    "@vdegenne/links": "^1.7.9",
    "@vdegenne/material-color-helpers": "file:../material-color-helpers",
    "@vdegenne/mini-rest": "^0.2.4",
    "@vdegenne/rollup-plugin-minify-html": "^1.0.6",
    "@vdegenne/router": "file:../router",
    "@vite-pwa/assets-generator": "^1.0.1",
    "custom-element-decorator": "^0.6.0",
    "html-vision": "^0.3.4",
    "lit": "^3.3.1",
    "lit-with-styles": "^2.1.3",
    "material-3-prompt-dialog": "^1.0.1",
    "material-shell": "^1.3.3",
    "rollup-plugin-material-all": "^1.2.2",
    "rollup-plugin-minify-template-literals": "^1.1.7",
    "snar": "^2.3.7",
    "snar-save-to-local-storage": "^1.0.1",
    "tailwindcss": "^4.1.13",
    "toastit": "^2.2.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-mdicon2svg": "^0.2.3",
    "vite-plugin-pwa": "^1.0.3",
    "vscode-ui-connector": "^0.2.4",
    "wireit": "^0.14.12"
  },
  "scripts": {
    "dev": "wireit",
    "build": "wireit",
    "pwa-assets": "wireit"
  },
  "wireit": {
    "dev": {
      "service": true,
      "command": "vite --host",
      "dependencies": [
        "vuc",
        "pwa-assets"
      ]
    },
    "build": {
      "command": "vite build",
      "files": [
        "./vite.config.js",
        "./src",
        "./index.html",
        "./public"
      ],
      "output": [],
      "dependencies": [
        "pwa-assets"
      ]
    },
    "vuc": {
      "service": true,
      "command": "vuc",
      "files": [
        "./.vuc.json"
      ]
    },
    "pwa-assets": {
      "command": "pwa-assets-generator --preset minimal-2023 --html.preset 2023 public/logo.svg",
      "files": [
        "public/logo.svg"
      ],
      "output": [
        "public/pwa-64x64.png",
        "public/pwa-192x192.png",
        "public/pwa-512x512.png",
        "public/maskable-icon-512x512.png",
        "public/apple-touch-icon-180x180.png",
        "public/favicon.ico"
      ]
    }
  }
}
